{
  "name": "vicentdev/cron-php",
  "description": "PHP cron replacement",
  "type": "project",
  "keywords": [
    "cron", "linux"
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Vicent Llinares",
      "email": "vicent@vicentdev.com"
    }
  ],
  "config": {
    "platform": {
      "php": "8.0.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "prefer-stable": true,
  "require": {
    "php": ">=8.0",
    "league/tactician": "^1.1",
    "pimple/pimple": "^3.4",
    "vlucas/phpdotenv": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "Cron\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cron\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
